<h1>New review</h1>
<%= simple_form_for([@restaurant, @review]) do |f| %>
  <%= f.input :rating %>
  <%= f.input :content %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>
<br>
<%= link_to "Back restaurant", restaurant_path(@restaurant) %>
<br>
<%= link_to "Back to list", restaurants_path %>
